        "title: \"Using Google's AlphaEarth Foundations Embeddings for Building Analysis: From Satellite Data to Predictive Urban Models\"\n",
        "author: \"Alex Singleton\"\n",
        "format:\n",
        "  html:\n",
        "    css: style.css\n",
        "    toc: true\n",
        "    toc-location: left\n",
        "    toc-float: true\n",
        "    theme: cosmo\n",
        "    highlight-style: github\n",
        "    reference-location: margin\n",
